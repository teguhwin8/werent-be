generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
}

model Reviews {
  id          Int     @id               @default(autoincrement()) 
  avatar_url  String  @db.VarChar(255)
  ratings     Int 
  measurament String  @db.VarChar(255)
  like_counts Int 
  comment     String 
  created_at  DateTime @default(now())
}
